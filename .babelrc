{
  "env": {
    "development": {
      "presets": ["es2015", "stage-1"],
      "plugins": [
       ["@babel/plugin-proposal-decorators", { "legacy": true }],
       ["@babel/plugin-proposal-class-properties", { "loose": true }],
       ["babel-plugin-styled-components"],
        ["transform-decorators-legacy"]
      ]
    },
    "production": {
      "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
      ],
      "plugins": [
       ["@babel/plugin-proposal-decorators", { "legacy": true }],
       ["@babel/plugin-proposal-class-properties", { "loose": true }],
       ["babel-plugin-styled-components"]
      ]
    }

  }
}
